marks=int(input("ENTER YOUR MARKS : "))
if marks>=0:
        if marks>=90 and marks<=100:
           print("GRADE A")
        elif marks>=80 and marks<=89:
           print("GRADE B")
        elif marks >=70 and marks<=79:
           print("GRADE C")
        elif marks>=60 and marks<=69:
           print("GRADE D")
        elif marks>=50 and marks<=59:
           print("GRADE E")
        else:
           print("GRADE F")      
else:
    print("wrong marks")
