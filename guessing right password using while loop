password=input("ENTER ANY PASSWORD : ")
passuser=input("ENTER THE PASSWORD : ")
if passuser==password:
        print("YOU GUESSED RIGHT ")
        print("GAME OVER")
else:
                while passuser!=password:
                        print("YOU GUESSED WRONG ! ENTER AGAIN ")
                        passuser=input("ENTER THE PASSWORD : ")
print("YOU GUESSED RIGHT ")
print("GAME OVER ")
